                    </div>
                    <!-- Project Requirements Tab End-->

                    <!-- Project Work Break Down Structure Tab Start-->
                    <div class="card">
                        <div class="card-header bg-primary">
                            <ul class="nav nav-tabs nav-tabs-neutral">
                                <li class="nav-item">
                                    <a class="nav-link active" data-toggle="tab" role="tab">
                    Project Work Break Down Structure
                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="card-body">
                            <table class="table table-hover table-responsive">
                                {% if currentUser %}
                                <thead class="text-primary">
                                    <th><strong>Group Name</strong></th>
                                    <th><strong>WBS Description</strong></th>
                                    <th><strong>Status</strong></th>
                                    <th><strong>Start Date</strong></th>
                                    <th><strong>End Date</strong></th>
                                    <th><strong>Create|Update</strong></th>
  																	<th><strong>Create|Update</strong></th>
                                </thead>
                                <tbody>
                                    {% for w in wbs %}
                                    <tr class="text-black">
                                        <td>{{w.groupName}}</td>
                                        <td>{{w.description}}</td>
                                        <td>{{w.status}}</td>
                                        <td>{{w.startdate}}</td>
                                        <td>{{w.enddate}}</td>
                                        <td>{{w.email}}</td>
  																			<td>{{w.updated_at}}</td>
                                        <td>
